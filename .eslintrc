{
    "parser": "babel-eslint",
    "env":{
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "space-before-blocks": "off",
        "indent":"off",
        "linebreak-style": "off",
        "eol-last":"off"

    },
    "extends": ["eslint:recommended", "google"]
    
}